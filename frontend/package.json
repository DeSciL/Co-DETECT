{
  "name": "autodetect",
  "description": "AutoDETECT: Automatic Discovery of Edge cases in TExt ClassificaTion",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && tsc -b && vite build",
    "prebuild": "npm run lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist --dotfiles --add"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/react-window": "^1.8.8",
    "antd": "^5.24.8",
    "d3": "^7.9.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^7.5.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
